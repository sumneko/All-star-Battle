set a=1
loop
exitwhen a > 255
if SubString(targetstr, b, b + 1) == SubString(originstr, a, a + 1) then
set result=result + a * numx
set a=256
endif
set a=a + 1
endloop
set b=b + 1
endloop
return result
endfunction
