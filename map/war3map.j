    if ((GetConvertedPlayerId(GetOwningPlayer(GetKillingUnitBJ())) >= 1) and (GetConvertedPlayerId(GetOwningPlayer(GetKillingUnitBJ())) <= 12) and (IsUnitEnemy(GetKillingUnitBJ(), GetOwningPlayer(GetDyingUnit())) == true)) then
