// Trigger: game time
//===========================================================================
function Trig_game_timeActions takes nothing returns nothing
    set bj_forLoopAIndex = 1
    set bj_forLoopAIndexEnd = 10
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        call AdjustPlayerStateBJ( 1, ConvertedPlayer(bj_forLoopAIndex), PLAYER_STATE_RESOURCE_FOOD_USED )
        if ((GetPlayerState(ConvertedPlayer(bj_forLoopAIndex), PLAYER_STATE_RESOURCE_FOOD_USED) >= 60)) then
            call SetPlayerStateBJ( ConvertedPlayer(bj_forLoopAIndex), PLAYER_STATE_RESOURCE_FOOD_USED, 0 )
            call AdjustPlayerStateBJ( 1, ConvertedPlayer(bj_forLoopAIndex), PLAYER_STATE_RESOURCE_LUMBER )
        else
        endif
        if ((udg_player[bj_forLoopAIndex] != null) and (IsUnitType(udg_player[bj_forLoopAIndex], UNIT_TYPE_DEAD) == false) and (GetPlayerController(ConvertedPlayer(bj_forLoopAIndex)) != MAP_CONTROL_COMPUTER) and (GetPlayerSlotState(ConvertedPlayer(bj_forLoopAIndex)) != PLAYER_SLOT_STATE_EMPTY) and (GetPlayerSlotState(ConvertedPlayer(bj_forLoopAIndex)) != PLAYER_SLOT_STATE_LEFT)) then
            set udg_guaji[bj_forLoopAIndex] = ( udg_guaji[bj_forLoopAIndex] + 1 )
            if ((udg_guaji[bj_forLoopAIndex] == 180)) then
                call DisplayTimedTextToForce( GetPlayersAll(), 10.00, ( ( udg_Color[bj_forLoopAIndex] + ( GetPlayerName(ConvertedPlayer(bj_forLoopAIndex)) + "|r" ) ) + ( "已经挂机了|cffffcc00" + ( "3" + "|r分钟" ) ) ) )
            else
            endif
            if ((udg_guaji[bj_forLoopAIndex] == 300)) then
                call DisplayTimedTextToForce( GetPlayersAll(), 10.00, ( ( udg_Color[bj_forLoopAIndex] + ( GetPlayerName(ConvertedPlayer(bj_forLoopAIndex)) + "|r" ) ) + ( "已经挂机了|cffffcc00" + ( "5" + "|r分钟，可输入|cffffcc00-kick|r指令将其踢出游戏" ) ) ) )
            else
            endif
            if ((udg_guaji[bj_forLoopAIndex] == 600)) then
                call DisplayTimedTextToForce( GetPlayersAll(), 10.00, ( ( udg_Color[bj_forLoopAIndex] + ( GetPlayerName(ConvertedPlayer(bj_forLoopAIndex)) + "|r" ) ) + ( "已经挂机了|cffffcc00" + ( "10" + "|r分钟，实在是太恶劣了！可输入|cffffcc00-kick|r指令将其踢出游戏" ) ) ) )
            else
            endif
        else
        endif
        if ((udg_player[bj_forLoopAIndex] != null)) then
            if ((IsItemOwned(GetItemOfTypeFromUnitBJ(udg_player[bj_forLoopAIndex], 'I019')) == true)) then
                call SetItemCharges( GetItemOfTypeFromUnitBJ(udg_player[bj_forLoopAIndex], 'I019'), IMinBJ(( GetItemCharges(GetItemOfTypeFromUnitBJ(udg_player[bj_forLoopAIndex], 'I019')) + 1 ), 50) )
            else
            endif
        else
        endif
        set bj_forLoopAIndex = bj_forLoopAIndex + 1
    endloop
endfunction

